@model IEnumerable<DalLib.Employee>
@{
    ViewBag.Title = "Home Page";
}
<table class=" table table-striped table-bordered">
    <thead>
        <tr>
            <th>
                EId

            </th>
            <th>Name</th>
            <th>Dept</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var emp in Model)
            {
                <tr>
                    <td>@emp.Id</td>
                    <td>@emp.Name</td>
                    <td>@emp.Dept</td>
                    <td>@Html.ActionLink("Edit", "EditEmployee", new { id = emp.Id })</td>
                    <td>@Html.ActionLink("Delete","DeleteEmployee", new{id=emp.Id})</td>
                </tr>
            }
    </tbody>

    
</table>
